generator client {
    provider        = "prisma-client-js"
    previewFeatures = []
    binaryTargets   = ["native", "linux-arm64-openssl-3.0.x"]

    output = "./generated/mongo_client"
}

datasource db {
    provider = "mongodb"
    url      = env("MONGOTEST_URL")
}

model Users {
    id       String @id @default(auto()) @map("_id") @db.ObjectId
    username String @unique
    password String
}
